{
  "name": "react-canvas-wrapper",
  "version": "0.2.6",
  "license": "MIT",
  "description": "React wrapper around a canvas element with a clean API for drawing.",
  "repository": "ryanhefner/react-canvas-wrapper",
  "author": "Ryan Hefner <hi@ryanhefner.com> (https://www.ryanhefner.com)",
  "homepage": "https://github.com/ryanhefner/react-canvas-wrapper#readme",
  "bugs": {
    "url": "https://github.com/ryanhefner/react-canvas-wrapper/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "canvas",
    "component"
  ],
  "files": [
    "es",
    "index.js",
    "src",
    "umd"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -f index.js && rm -fr es && rm -fr umd",
    "prebuild": "npm run clean",
    "build": "node ./tools/build.js",
    "watch": "babel ./src -d . --ignore __tests__ --watch",
    "prepare": "npm run build",
    "prepublishOnly": "node ./tools/build.js",
    "push-release": "git push origin master && git push --tags",
    "test": "jest"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "dependencies": {
    "clean-react-props": "^0.2.6",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "greenkeeper-lockfile": "^1.15.1",
    "gzip-size": "^5.0.0",
    "jest": "^23.4.2",
    "jest-enzyme": "^6.0.2",
    "pretty-bytes": "^5.1.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "^4.3.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "testURL": "http://localhost"
  }
}
