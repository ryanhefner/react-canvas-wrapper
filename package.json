{
  "name": "react-canvas-wrapper",
  "version": "0.4.3",
  "license": "MIT",
  "description": "React wrapper around a canvas element with a clean API for drawing.",
  "repository": "ryanhefner/react-canvas-wrapper",
  "author": "Ryan Hefner <hi@ryanhefner.com> (https://www.ryanhefner.com)",
  "homepage": "https://github.com/ryanhefner/react-canvas-wrapper#readme",
  "bugs": {
    "url": "https://github.com/ryanhefner/react-canvas-wrapper/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "canvas",
    "component"
  ],
  "files": [
    "es",
    "index.js",
    "src",
    "umd"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -f index.js && rm -fr es && rm -fr umd",
    "prebuild": "npm run clean",
    "build": "node ./tools/build.js",
    "watch": "babel ./src -d . --ignore __tests__ --watch",
    "prepare": "npm run build",
    "prepublishOnly": "node ./tools/build.js",
    "push-release": "git push origin master && git push --tags",
    "test": "jest"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "dependencies": {
    "clean-react-props": "^0.4.0",
    "nanoid": "^2.1.11",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "braces": ">=2.3.1",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "greenkeeper-lockfile": "^1.15.1",
    "gzip-size": "^5.1.1",
    "jest": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "pretty-bytes": "^5.3.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^2.0.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "testURL": "http://localhost"
  }
}
